{
    "name": "electron-react-template",
    "version": "0.0.0",
    "author:": "",
    "description": "Electron React Template",
    "main": "build/app/main.js",
    "scripts": {
        "start": "grunt & electron .",
        "pack": "grunt & electron-builder --dir -w --x64"
    },
    "dependencies": {
        "jquery": "^3.2.1",
        "react": "^15.3.2",
        "react-dom": "^15.3.2"
    },
    "devDependencies": {
        "babel-preset-react": "^6.24.1",
        "electron": "^1.6.11",
        "electron-builder": "^19.16.0",
        "grunt": "^1.0.1",
        "grunt-babel": "^6.0.0",
        "grunt-contrib-copy": "^1.0.0",
        "grunt-contrib-sass": "^1.0.0"
    },
    "babel": {
        "plugins": [
            "transform-react-jsx"
        ]
    },
    "build": {
        "files": [
            "build/app/**/**.*"
        ]
    }
}
